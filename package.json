{
  "name": "chokidar-watcher",
  "description": "File system watcher based on chokidar that emits add/change/rename/unlink events.",
  "version": "1.4.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck && tstei",
    "compile:watch": "tsc --skipLibCheck --watch",
    "prepublishOnly": "npm run clean && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/chokidar-watcher/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/chokidar-watcher.git"
  },
  "keywords": [
    "file-system",
    "watcher",
    "chokidar",
    "add",
    "change",
    "rename",
    "unlink"
  ],
  "engines": {
    "node": ">= 10"
  },
  "dependencies": {
    "@types/chokidar": "^2.1.3"
  },
  "peerDependencies": {
    "chokidar": "*"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "chokidar": "^3.3.1",
    "rimraf": "^2.6.3",
    "typescript": "^3.8.3",
    "typescript-transform-export-interop": "^1.0.2"
  }
}
